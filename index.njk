---
maxResults: 14
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>Speedlify</title>
		<link rel="stylesheet" href="style.css">
		<link rel="stylesheet" href="chartist.css">
	</head>
	<body>
		<h1>Speedlify</h1>
		<p>Benchmark web pages over time (ordered by Lighthouse scores). Updates <em>at most</em> once an hour and automatically once a day. Only the last {{ maxResults }} result{% if maxResults != 1 %}s{% endif %} are shown.</p>
		<ol>
			{%- for site in results | sortObject([':lastkey', 'ranks', 'hundos']) | reverse %}
			{%- set key = site | getObjectKey("last") %}
			{%- set hundos = site[key] | hundoCount %}
			<li><a href="#{{ site[key].url | shortHash }}"><code class="url">{{ site[key].url | displayUrl }}</code></a>{% if loop.index0 === 0 %} üèÜ{% endif %} <span class="count">{% if hundos === 4 %}üòç FOUR HUNDOS üíØüíØüíØüíØ{% else %}{{ hundos }} ({{ site[key] | lighthouseTotal }} Total){% endif %}</span></li>
			{%- endfor %}
		</ol>

		{%- for site in results | sortObject %}
			{%- set key = site | getObjectKey %}
			<details open>
				<summary id="{{ site[key].url | shortHash }}"><code class="url">{{ site[key].url | displayUrl }}</code> <a href="{{ site[key].url }}" class="external">Go ‚Üí</a></summary>
				<div class="layout">
					<div class="tablewrap">
						<table data-make-chart class="results">
							<thead>
								<tr>
									<th rowspan="2" class="results-date">Date</th>
									<th rowspan="2">Rank</th>
									<th colspan="4">Lighthouse</th>
									<th>Axe</th>
								</tr>
								<tr>
									<th class="results-series-a"><abbr title="Performance">Perf</abbr></th>
									<th class="results-series-b"><abbr title="Accessibility">A11Y</abbr></th>
									<th class="results-series-c">Best Practices</th>
									<th class="results-series-d">SEO</th>
									<th>Violations</th>
								</tr>
							</thead>
							<tbody>
							{%- for entry in site | sortObject | head(maxResults) %}
								<tr>
									<td><code class="date">{{ entry.timestamp | displayDate }}</code></td>
									<td>{% if entry.ranks.hundos %}{% if entry.ranks.hundos === 1 %}üèÜ {% endif %}#{{ entry.ranks.hundos }}{% endif %}</td>
									<td data-numeric-value="{{ entry.lighthouse.performance * 100 }}">{{ entry.lighthouse.performance | displayTableCellValue }}</td>
									<td data-numeric-value="{{ entry.lighthouse.accessibility * 100 }}">{{ entry.lighthouse.accessibility | displayTableCellValue }}</td>
									<td data-numeric-value="{{ entry.lighthouse.bestPractices * 100 }}">{{ entry.lighthouse.bestPractices | displayTableCellValue }}</td>
									<td data-numeric-value="{{ entry.lighthouse.seo * 100 }}">{{ entry.lighthouse.seo | displayTableCellValue }}</td>
									<td>{% if entry.axe.violations == 0 %}‚úÖ{% else %}üö´ {{ entry.axe.violations }}{% endif %}</td>
								</tr>
							{%- endfor %}
							</tbody>
						</table>
					</div>
					<div class="ct-chart ct-double-octave"></div>
				</div>

				<div class="layout">
					<div class="tablewrap">
						<table data-make-chart class="results">
							<thead>
								<tr>
									<th rowspan="2" class="results-date">Date</th>
									<th colspan="4">Core Web Vitals</th>
									<th colspan="3">Web Vitals</th>
								</tr>
								<tr>
									<th class="results-series-a"><abbr title="Largest Contentful Paint">LCP</abbr></th>
									<th class="results-series-b">Max Potential <abbr title="First Input Delay">FID</abbr></th>
									<th class="results-series-c"><abbr title="Total Blocking Time">TBT</abbr></th>
									<th><abbr title="Cumulative Layout Shift">CLS</abbr></th>

									<th class="results-series-d"><abbr title="First Contentful Paint">FCP</abbr></th>
									<th class="results-series-e"><abbr title="Speed Index">SI</abbr></th>
									<th class="results-series-f"><abbr title="Time to Interactive">TTI</abbr></th>
								</tr>
							</thead>
							<tbody>
							{%- for entry in site | sortObject | head(maxResults) %}
								<tr>
									<td><code class="date">{{ entry.timestamp | displayDate }}</code></td>
									<td data-numeric-value="{{ entry.largestContentfulPaint }}">{% if entry.largestContentfulPaint <= 1550 %}‚úÖ {% endif %}{{ entry.largestContentfulPaint | digits(0) }}ms</td>
									{# https://web.dev/lighthouse-max-potential-fid/#how-lighthouse-determines-your-max-potential-fid-score #}
									<td data-numeric-value="{{ entry.maxPotentialFirstInputDelay }}">{% if entry.maxPotentialFirstInputDelay <= 130 %}‚úÖ {% endif %}{{ entry.maxPotentialFirstInputDelay | digits(0) }}ms</td>
									<td data-numeric-value="{{ entry.totalBlockingTime }}">{% if entry.totalBlockingTime <= 130 %}‚úÖ {% endif %}{{ entry.totalBlockingTime | digits(0) }}ms</td>
									<td>{% if entry.cumulativeLayoutShift <= 0.03 %}‚úÖ {% endif %}{{ entry.cumulativeLayoutShift }}</td>
									<td data-numeric-value="{{ entry.firstContentfulPaint }}">{% if entry.firstContentfulPaint <= 1350 %}‚úÖ {% endif %}{{ entry.firstContentfulPaint | digits(0) }}ms</td>
									<td data-numeric-value="{{ entry.speedIndex }}">{% if entry.speedIndex <= 1960 %}‚úÖ {% endif %}{{ entry.speedIndex | digits(0) }}ms</td>
									<td data-numeric-value="{{ entry.timeToInteractive }}">{% if entry.timeToInteractive <= 1940 %}‚úÖ {% endif %}{{ entry.timeToInteractive | digits(0) }}ms</td>
								</tr>
							{%- endfor %}
							</tbody>
						</table>
					</div>
					<div class="ct-chart ct-double-octave"></div>
				</div>

				<div class="layout">
					<div class="tablewrap">
						<table data-make-chart class="results">
							<thead>
								<tr>
									<th rowspan="2" class="results-date">Date</th>
									<th colspan="5">Weight</th>
								</tr>
								<tr>
									<th class="results-series-a">Total</th>
									<th class="results-series-b">HTML</th>
									<th class="results-series-c"><abbr title="JavaScript">JS</abbr></th>
									<th class="results-series-d">Images</th>
									<th class="results-series-e">Fonts</th>
								</tr>
							</thead>
							<tbody>
							{%- for entry in site | sortObject | head(maxResults) %}
								<tr>
									<td><code class="date">{{ entry.timestamp | displayDate }}</code></td>
									<td data-numeric-value="{{ entry.weight.total / 1024 }}">{{ entry.weight.total | displayFilesize }}</td>
									<td data-numeric-value="{{ entry.weight.document / 1024 }}">{{ entry.weight.document | displayFilesize }}</td>
									<td data-numeric-value="{{ entry.weight.script / 1024 }}">{{ entry.weight.script | displayFilesize }} <span class="count">√ó{{ entry.weight.scriptCount }}</span></td>
									<td data-numeric-value="{{ entry.weight.image / 1024 }}">{{ entry.weight.image | displayFilesize }} <span class="count">√ó{{ entry.weight.imageCount }}</span></td>
									<td data-numeric-value="{{ entry.weight.font / 1024 }}">{{ entry.weight.font | displayFilesize }} <span class="count">√ó{{ entry.weight.fontCount }}</span></td>
								</tr>
							{%- endfor %}
							</tbody>
						</table>
					</div>
					<div class="ct-chart ct-double-octave"></div>
				</div>
			</details>
		{%- endfor %}

		<footer>
			MIT licensed. Source code at <a href="https://github.com/zachleat/speedlify/"><code>github.com/zachleat/speedlify/</code></a>
		</footer>

		<script src="chartist.js"></script>
		<script src="chart.js"></script>
	</body>
</html>